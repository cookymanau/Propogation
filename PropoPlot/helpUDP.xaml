<Window x:Class="PropoPlot.helpUDP"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PropoPlot"
        mc:Ignorable="d"
        Title="helpUDP" Height="550" Width="500">
    <Border BorderThickness="10">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>

        <TextBlock x:Name="helpUDPText" Grid.Row="0"/>
        <Button x:Name="helpUDPExit" Grid.Row="1" Content="Close" Click="helpUDPExit_Click" />

    </Grid>
    </Border>
</Window>
